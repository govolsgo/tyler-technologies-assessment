@page
@model Tyler_Tech_Test.Pages.subordinate_viewerModel
@{
    ViewData["Title"] = "Subordinate Viewer";
}

<title>S-Squared Enterprises Personnel System</title>
<h1>S-Squared Enterprises Personnel System</h1>

<label for="manager-dropdown">Manager: </label>
<select name="manager-dropdown"></select>
@{
    foreach (var manager in Model.managerList)
    {
        <option id="@manager.ID">@manager.LastName, @manager.FirstName</option>
    }
}

<table>
    <tr>
        <th>Employee ID</th>
        <th>Last Name</th>
        <th>First Name</th>
    </tr>
</table>

<button id="add-button" name="add-button">Add Employee</button>